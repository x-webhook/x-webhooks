{
    "name": "xwebhook/xwebhook",
    "type": "library",
    "description": "Xwebhook PHP Library",
    "keywords": ["webhooks","api"],
    "homepage": "https://www.xwebhook.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Xwebhook",
            "email": "support@xwebhook.com",
            "homepage": "http://www.xwebhook.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*"
      },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.89"
    },
    "autoload": {
        "psr-4": {
            "Xwebhook\\": "php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xwebhook\\": [
                "php/tests/"
            ]
        }
    }
}
